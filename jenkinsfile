pipeline {
    agent any

    stages {
        stage('clone') {
            steps {
               git branch: 'main', url: 'https://github.com/CloudTechDevOps/project-1-maven-jenkins-CICD-docker-eks-.git'
            }
        }
         stage('He-1') {
            steps {
               sh 'mvn clean'
            }
        }
         stage('He-2') {
            steps {
               sh 'mvn install'
            }
        }
         stage('he-3') {
            steps {
              deploy adapters: [tomcat9(credentialsId: 'tomcat', path: '', url: 'http://35.154.11.23:8085/')], contextPath: null, war: '**/*.war'
            }
        }
    }
}
